{
    "name": "beyondcode/slack-notification-channel",
    "description": "Slack Notification Channel for Laravel using API tokens.",
    "keywords": ["laravel", "notifications", "slack"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de"
        }
    ],
    "require": {
        "php": "^7.3.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\SlackNotificationChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\SlackNotificationChannel\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "BeyondCode\\SlackNotificationChannel\\SlackChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
